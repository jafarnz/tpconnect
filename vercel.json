{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "buildCommand": "npx prisma migrate deploy && npx prisma generate && npm install",
  "installCommand": "npm install && npm run dev",
  "framework": "nextjs"
}
